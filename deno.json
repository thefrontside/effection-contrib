  },
  "lint": {
    "rules": {
      "exclude": [
        "prefer-const",
        "require-yield"
      ]
    }
  },
  "workspace": [
    "./deno-deploy",
    "./jsonl-store",
    "./fx",
    "./task-buffer",
    "./tinyexec",
    "./timebox",
    "./websocket"
  ],
  "deploy": {
    "project": "aa1dbfaa-d7c1-49d7-b514-69e1d8344f95",
    "exclude": [
      "**/node_modules"
    ],
    "include": [
      "."
    ],
    "entrypoint": "www/main.ts"
  }
}
